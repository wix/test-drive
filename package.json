{
  "name": "test-drive",
  "version": "0.6.2",
  "description": "Opinionated library for writing web component tests",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "prepublish": "yarn build",
    "test": "yarn test:node && yarn test:browser",
    "test:node": "mocha --require ts-node/register ./test/index.node.ts --watch-extensions ts",
    "test:browser": "karma start --single-run",
    "start": "webpack-dev-server --progress"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.7.0",
    "chai-style": "^1.0.3",
    "dom-matches": "^2.0.0",
    "sinon": "^5.0.4",
    "sinon-chai": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-dom": "^0.0.6",
    "@types/mocha": "^5.2.0",
    "@types/sinon": "^4.3.1",
    "@types/sinon-chai": "^2.7.29",
    "core-js": "^2.5.5",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.1.1",
    "mocha-loader": "^1.1.3",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.2",
    "typescript": "~2.8.3",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "files": [
    "dist/src",
    "helpers",
    "matchers.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/test-drive.git"
  },
  "author": "Jiri Tobisek <jiri@wix.com>",
  "contributors": [
    "Avi Vahl <avi.vahl@wix.com>",
    "Amir Arad <amira@wix.com>",
    "Kieran Williams <kieranw@wix.com>",
    "Lior Zisman <liorzi@wix.com>"
  ],
  "bugs": {
    "url": "https://github.com/wix/test-drive/issues"
  },
  "homepage": "https://github.com/wix/test-drive#readme",
  "license": "MIT"
}
